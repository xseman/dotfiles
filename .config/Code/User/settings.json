{
    "editor.autoClosingBrackets": "always",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll.markdownlint": true
    },
    "editor.codeLens": true,
    "editor.columnSelection": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 2,
    "editor.detectIndentation": true,
    "editor.folding": true,
    "editor.foldingHighlight": false,
    "editor.fontFamily": "'JetBrains Mono', 'Input Mono', 'FontAwesome', 'Material Design Icons Desktop'",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.fontWeight": "500",
    "editor.formatOnSave": false,
    "editor.glyphMargin": false,
    "editor.insertSpaces": true,
    "editor.largeFileOptimizations": false,
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "selection",
    "editor.rulers": [
        80
    ],
    "editor.semanticHighlighting.enabled": true,
    "editor.smoothScrolling": false,
    "editor.snippetSuggestions": "inline",
    "editor.suggestSelection": "first",
    "editor.tabSize": 4,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.wordWrap": "off",

    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "IDEA like light Theme",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.showTabs": true,
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.keyboardNavigation": "filter",
    "workbench.settings.editor": "json",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": true,
    "workbench.tree.indent": 20,
    "workbench.tree.renderIndentGuides": "always",

    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "window.title": "${rootName}",
    "window.titleBarStyle": "native",

    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "on",

    "explorer.autoReveal": false,
    "explorer.incrementalNaming": "smart",
    "explorer.sortOrder": "type",

    "files.autoSave": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        /** defaults */
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },

    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.experimentalLinkProvider": true,
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.1,
    "terminal.integrated.rendererType": "experimentalWebgl",
    "terminal.external.linuxExec": "/usr/bin/alacritty",
    "terminal.integrated.shell.linux": "/usr/bin/fish",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    // "terminal.integrated.env.linux": {
    //     "WORKSPACE": "${workspaceFolder}"
    // },
    // "terminal.integrated.shellArgs.linux": [
    //     "--command", "/usr/bin/fish",
    //     // dynamicly change env based on opened workspace window
    //     // combination with "terminal.integrated.env.linux"
    //     "--command", "set WORKSPACE=$WORKSPACE",
    //     "--command", "tmux set-enviroment -g WORKSPACE $WORKSPACE",
    //     "--command", "tmux new-session -A -s main"
    // ],

    "debug.console.wordWrap": false,
    "debug.console.fontSize": 11,

    "problems.autoReveal": false,

    "zenMode.centerLayout": true,
    "zenMode.fullScreen": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,

    "python.defaultInterpreterPath": "usr/bin/python3",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.formatting.autopep8Args": [
        "--aggressive",
        "--in-place",
        "--max-line-length",
        "80"
    ],

    "java.configuration.checkProjectSettingsExclusions": false,
    "java.project.importOnFirstTimeStartup": "automatic",
    "java.refactor.renameFromFileExplorer": "preview",
    "java.semanticHighlighting.enabled": true,
    "java.configuration.runtimes": [
        // {
        //     "name": "JavaSE-1.8",
        //     "path": "/home/filip/.jdks/adopt-openjdk-1.8.0_265",
        // },
        {
            "name": "JavaSE-11",
            "path": "/usr/lib/jvm/java-1.11.0-openjdk-amd64"
        },
        {
            "name": "JavaSE-14",
            "path": "/usr/lib/jvm/java-1.14.0-openjdk-amd64"
        }
    ],

    "javascript.format.semicolons": "insert",
    "javascript.preferences.quoteStyle": "double",
    "javascript.suggest.includeAutomaticOptionalChainCompletions": true,

    "typescript.format.semicolons": "insert",
    "typescript.preferences.quoteStyle": "auto",
    "typescript.preferences.useAliasesForRenames": false,
    "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
    "typescript.updateImportsOnFileMove.enabled": "always",

    "typescriptHero.imports.grouping": ["Plains", "Modules", "Workspace"],
    "typescriptHero.imports.multiLineWrapThreshold": 80,
    "typescriptHero.imports.organizeOnSave": false,
    "typescriptHero.imports.stringQuoteStyle": "'",

    "prettier.printWidth": 80,
    "prettier.proseWrap": "always",
    "prettier.singleQuote": false,
    "prettier.useTabs": false,
    "prettier.tabWidth": 4,

    "eslint.codeActionsOnSave.mode": "all",
    "eslint.alwaysShowStatus": true,
    "eslint.run": "onSave",
    "eslint.validate": [
        "css",
        "html",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],

    "scss.lint.emptyRules": "ignore",
    "less.lint.emptyRules": "ignore",
    "css.lint.emptyRules": "ignore",

    "markdownlint.config": {
        "default": true,
        "spaces": true,
        "line-length": true,
        "MD036": false,
        "MD007": { "indent": 4 }
    },

    "emmet.excludeLanguages": ["markdown", "typescript"],
    "emmet.includeLanguages": {
        "typescript": "typescriptreact",
        "javascript": "javascriptreact"
    },

    "git.confirmSync": false,
    "git.suggestSmartCommit": false,
    "git.untrackedChanges": "separate",
    "git.openDiffOnClick": true,

    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.views.repositories.files.layout": "tree",

    "diffEditor.renderSideBySide": true,
    "diffEditor.maxComputationTime": 0,
    "diffEditor.ignoreTrimWhitespace": false,

    "extensions.autoCheckUpdates": true,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": true,

    "xml.format.emptyElements": "collapse",
    "xml.format.joinCommentLines": true,
    "xml.format.joinContentLines": true,
    "xml.format.preserveAttributeLineBreaks": true,
    "xml.format.spaceBeforeEmptyCloseTag": true,
    "xml.format.splitAttributes": true,
    "xml.format.xsiSchemaLocationSplit": "onElement",

    "i18n-ally.displayLanguage": "sk",
    "i18n-ally.keystyle": "nested",

    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,

    "npm-intellisense.importQuotes": "\"",

    "svg.preview.mode": "svg",

    "mdb.sendTelemetry": false,

    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[fish]": {
        "editor.defaultFormatter": "bmalehorn.vscode-fish",
        "editor.tabSize": 4
    },
    "[go]": {
        "editor.codeActionsOnSave": {
            "source.fixAll": true,
            "source.organizeImports": true
        },
        "editor.formatOnSave": true,
        "editor.tabSize": 4
    },
    "[handlebars]": {
        "editor.formatOnSave": false,
        "editor.tabSize": 4
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
    },
    "[java]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4
        // "editor.codeActionsOnSave": {
        //     "source.organizeImports": true
        // },
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
        "editor.formatOnSave": false
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": false
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
    },
    "[less]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
    },
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
        "editor.formatOnSave": true,
        "editor.wordWrap": "off"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.formatOnSave": true
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
    },
    "[shellscript]": {
        "editor.formatOnSave": false,
        "editor.tabSize": 4
    },
    "[tex]": {
        "editor.tabSize": 4
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
        // "editor.defaultFormatter": "vscode.typescript-language-features",
        // "editor.formatOnSave": true,
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": false
        // "editor.defaultFormatter": "vscode.typescript-language-features",
        // "editor.formatOnSave": true,
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml",
        "editor.tabSize": 4
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": false,
        "editor.tabSize": 2
    },
    "[yml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": false,
        "editor.tabSize": 2
    },
}
